unit Model.Biblioteca.Functions;

interface

uses
    IniFiles
  , System.SysUtils
  ;

  procedure SetValorIni(pLocal, pSessao, pSubSessao : String; pValor : String);

implementation


procedure SetValorIni(pLocal, pSessao, pSubSessao : String; pValor : String);
var
  vArquivo : TIniFile;
begin
  vArquivo := TIniFile.Create(ExtractFilePath());
end;

end.
